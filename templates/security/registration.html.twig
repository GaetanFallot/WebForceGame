{% extends 'base.html.twig' %}

{% block body %}
    <h1> Inscription</h1>
    {{ form_start(form) }}

    {{ form_row(form.firstname) }}

    {{ form_row(form.lastname) }}

    {{ form_row(form.user_name) }}

    {{ form_row(form.email) }}

    {{ form_row(form.password.first) }}
    
    {{ form_row(form.password.second) }}

    <button type="submit">S'inscrire</button>
    {{ form_end(form) }}

{% endblock %}